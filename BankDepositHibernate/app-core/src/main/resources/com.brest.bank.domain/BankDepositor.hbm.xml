<?xml version='1.0'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.brest.bank.domain">

    <class name="BankDepositor" table="BANKDEPOSITOR">
        <id name="depositorId" column="depositorId">
            <generator class="native"/>
        </id>
        <property name="depositorName" type="String" column="depositorName"/>
        <property name="depositorIdDeposit" type="Long" column="depositorIdDeposit"/>
        <property name="depositorDateDeposit" type="Date" column="depositorDateDeposit"/>
        <property name="depositorAmountDeposit" type="Integer" column="depositorAmountDeposit"/>
        <property name="depositorAmountPlusDeposit" type="Integer" column="depositorAmountPlusDeposit"/>
        <property name="depositorAmountMinusDeposit" type="Integer" column="depositorAmountMinusDeposit"/>
        <property name="depositorDateReturnDeposit" type="Date" column="depositorDateReturnDeposit"/>
        <property name="depositorMarkReturnDeposit" type="Integer" column="depositorMarkReturnDeposit"/>

        <many-to-one name="bankDeposit" class="com.brest.bank.domain.BankDeposit">
            <column name="depositorIdDeposit" not-null="true"/>
        </many-to-one>

    </class>

</hibernate-mapping>